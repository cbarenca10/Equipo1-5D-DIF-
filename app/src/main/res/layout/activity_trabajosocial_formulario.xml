<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/register_final"
    tools:context=".trabajosocial_formulario">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Icons"
        android:id="@+id/TopButtonPanel">

        <TextView
            android:id="@+id/TextUno"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Habitantes"
            android:layout_marginTop="20dp"
            android:textAlignment="center"
            android:layout_weight="1"
            android:textSize="25dp"
            android:gravity="center_horizontal"></TextView>
    </LinearLayout>

    <TextView
        android:id="@+id/TextDos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/TopButtonPanel"
        android:text="Estudio Socioeconomico"
        android:textAlignment="center"
        android:layout_marginTop="30dp"
        android:textSize="18sp"
        android:gravity="center_horizontal"></TextView>
    <EditText
        android:id="@+id/EstadoCatcher"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/TextDos"
        android:layout_marginTop="10dp"
        android:hint="Ingresar Estado"></EditText>

    <TextView
        android:id="@+id/TextTres"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/EstadoCatcher"
        android:text="Beneficiario"
        android:textAlignment="center"
        android:layout_marginTop="30dp"
        android:textSize="18sp"
        android:gravity="center_horizontal"></TextView>
    <TextView
        android:id="@+id/TextCuatro"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/TextTres"
        android:text="Fecha de Nacimiento"
        android:textAlignment="center"
        android:layout_marginTop="30dp"
        android:textSize="18sp"
        android:gravity="center_horizontal"></TextView>

    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/FechadeNacimientoCatcher"
        android:layout_below="@+id/TextCuatro"></Spinner>

    <EditText
        android:id="@+id/ParentescoCatcher"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/FechadeNacimientoCatcher"
        android:layout_marginTop="15dp"
        android:hint="Ingresar Parentesco"></EditText>
    <EditText
        android:id="@+id/EscolaridadCatcher"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/ParentescoCatcher"
        android:layout_marginTop="15dp"
        android:hint="Ingresar Escolaridad"></EditText>
    <EditText
        android:id="@+id/OcupacionCatcher"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/EscolaridadCatcher"
        android:layout_marginTop="15dp"
        android:hint="Ingresar Ocupacion"></EditText>
    <EditText
        android:id="@+id/IngresoMensualCatcher"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/OcupacionCatcher"
        android:layout_marginTop="15dp"
        android:hint="Ingresar Ingreso Mensual"></EditText>/>
</RelativeLayout>